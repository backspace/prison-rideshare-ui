{{high-charts content=data chartOptions=options theme=theme callback=(action 'afterRenderCallback')}}

{{#if rendered}}
  {{#ember-wormhole to='grouping-weeks'}}
    {{paper-button label='Weeks' primary=(eq grouping 'weeks') onClick=(action (mut grouping) 'weeks')}}
  {{/ember-wormhole}}
  {{#ember-wormhole to='grouping-months'}}
    {{paper-button label='Months' primary=(eq grouping 'months') onClick=(action (mut grouping) 'months')}}
  {{/ember-wormhole}}
{{/if}}
